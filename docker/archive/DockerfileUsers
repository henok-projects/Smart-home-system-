FROM openjdk:17
LABEL authors="Galsie"

# Set the working directory
WORKDIR /app

# Copy your JAR files to the image
COPY ../../users-service/target/users-service-0.0.1-SNAPSHOT.jar /app


# Expose the ports for your applications
EXPOSE 60136


CMD ["java", "-jar", "users-service-0.0.1-SNAPSHOT.jar"]
