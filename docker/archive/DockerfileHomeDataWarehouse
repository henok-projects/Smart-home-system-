FROM openjdk:17
LABEL authors="Galsie"

# Set the working directory
WORKDIR /app

# Copy your JAR files to the image
COPY ../../homes-datawarehouse-service/target/homes-datawarehouse-service-0.0.1-SNAPSHOT.jar /app



# Expose the ports for your applications
EXPOSE 60133


CMD ["java", "-jar", "homes-datawarehouse-service-0.0.1-SNAPSHOT.jar"]
