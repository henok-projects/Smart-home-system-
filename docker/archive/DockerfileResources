FROM openjdk:17
LABEL authors="Galsie"

# Set the working directory
WORKDIR /app

# Copy your JAR files to the image
COPY ../resources-service/target/resources-service-0.0.1-SNAPSHOT.jar /app


# Expose the ports for your applications
EXPOSE 60132


CMD ["java", "-jar", "resources-service-0.0.1-SNAPSHOT.jar"]
