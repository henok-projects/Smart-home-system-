FROM openjdk:17
LABEL authors="Galsie"

# Set the working directory
WORKDIR /app

# Copy your JAR files to the image
COPY ../../homes-service/target/homes-service-0.0.1-SNAPSHOT.jar /app


# Expose the ports for your applications
EXPOSE 60134


CMD ["java", "-jar", "homes-service-0.0.1-SNAPSHOT.jar"]
